`timescale 1us/100ns
module barrel_shifter(water, shift, wine);
  input [31:0] water; //32-Bit input 
  input shift; //input activate mutex
  output [31:0] wine; //32-Bit output

  //so many god damn mutexs
  //mux_2to1 name (input1 , input2, output, on/off)
  mux_2to1 m0 (1'b0, water[0], wine[0], shift); //input1:0 input2:water[0] output:wine[0] mutex switch: shift
  mux_2to1 m1 (water[0], water[1], wine[1], shift);
  mux_2to1 m2 (water[1], water[2], wine[2], shift);
  mux_2to1 m3 (water[2], water[3], wine[3], shift);
  mux_2to1 m4 (water[3], water[4], wine[4], shift);
  mux_2to1 m5 (water[4], water[5], wine[5], shift);
